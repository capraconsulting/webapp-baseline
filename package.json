{
  "name": "webapp-baseline",
  "version": "0.0.1",
  "description": "Baseline for creating React+Typescript web apps",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --env.production",
    "build:measure": "MEASURE=true webpack --env.production",
    "build:analyze": "ANALYZE=true webpack --env.production",
    "test": "jest --coverage src",
    "test:watch": "jest src --watch",
    "test:e2e": "npx testcafe chrome e2e/tests/**/*.test.ts",
    "test:e2e:jenkins": "npx testcafe --app './scripts/serve-dist.sh' chrome:headless e2e/tests/**/*.test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/jest-each": "24.3.0",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "autoprefixer": "9.8.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "git-revision-webpack-plugin": "3.0.6",
    "html-webpack-plugin": "4.3.0",
    "http-server": "0.12.3",
    "jest": "26.0.1",
    "jest-each": "26.0.1",
    "jest-sonar-reporter": "2.0.0",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.5",
    "source-map-loader": "1.0.0",
    "speed-measure-webpack-plugin": "1.3.3",
    "style-loader": "1.2.1",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "browserlist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  }
}
